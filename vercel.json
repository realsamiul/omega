{
  "rewrites": [
    {
      "source": "/",
      "destination": "/space-sustainability.html"
    },
    {
      "source": "/space-sustainability/:path*",
      "destination": "/space-sustainability/:path*"
    }
  ],
  "headers": [
    {
      "source": "/space-sustainability/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
